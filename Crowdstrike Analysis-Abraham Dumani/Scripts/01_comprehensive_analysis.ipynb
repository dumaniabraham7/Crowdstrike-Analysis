    "            \n",
    "            # Balance Sheet\n",
    "            balance_sheet = self.stock.balance_sheet\n",
    "            if balance_sheet is not None:\n",
    "                balance_sheet.to_csv('data/raw/balance_sheet.csv')\n",
    "                self.data['balance_sheet'] = balance_sheet\n",
